<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".features.registerGame.searchGame.SearchGameFragment"
        android:background="@color/colorPrimary">

    <include layout="@layout/logo_tentacle"/>

    <br.com.concrete.tentacle.custom.ListCustom
        android:id="@+id/listCustom"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_0"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:icon="@drawable/ilustra_tentacle"
        app:layoutManager="LinearLayoutManager"
        app:buttonNameError="@string/load_again"
        app:errorDescription="@string/first_game_register"
        app:buttonNameAction="@string/did_not_find_what_you_wanted"
        app:endLessScroll="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>